{
  "version": 2,
  "name": "ai-notebook-fullstack",
  "builds": [
    {
      "src": "ai-notebook/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "buildCommand": "npm run build"
      }
    },
    {
      "src": "ai-notebook/backend/api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/ai-notebook/backend/api/index.py",
      "methods": ["GET"]
    },
    {
      "src": "/api/(.*)",
      "dest": "/ai-notebook/backend/api/index.py"
    },
    {
      "src": "/api",
      "dest": "/ai-notebook/backend/api/index.py"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/ai-notebook/frontend/dist/assets/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|json|txt))",
      "dest": "/ai-notebook/frontend/dist/$1",
      "headers": {
        "Cache-Control": "public, max-age=3600"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/ai-notebook/frontend/dist/index.html"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task/backend",
    "FLASK_ENV": "production",
    "DATABASE_URL": "sqlite:///ai_notebook.db"
  }
}