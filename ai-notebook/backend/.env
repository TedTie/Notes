# Flask配置
SECRET_KEY=your-secret-key-here-change-in-production
FLASK_ENV=development
FLASK_DEBUG=True

# 数据库配置
DATABASE_URL=sqlite:///database.db

# OpenRouter API配置（用户需要在设置中配置）
# OPENROUTER_API_KEY=your-openrouter-api-key-here

# 服务器配置
HOST=0.0.0.0
PORT=5000

# 加密配置
# 用于API密钥加密的主密钥（生产环境中请使用强密钥）
ENCRYPTION_KEY=your-encryption-master-key-change-in-production-32-chars

# 安全配置
# 启用API密钥日志过滤
ENABLE_LOG_FILTERING=True
# 启用速率限制
ENABLE_RATE_LIMITING=True