# Supabase配置
# 请复制此文件为.env.local并填入实际的Supabase配置信息

# Supabase项目URL
# 格式: https://your-project-ref.supabase.co
VITE_SUPABASE_URL=your_supabase_project_url

# Supabase匿名密钥 (公开密钥，可以安全地在前端使用)
# 从Supabase项目设置 > API > anon public 获取
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# 应用配置
VITE_APP_NAME=AI Notebook
VITE_APP_VERSION=1.0.0

# 开发环境配置
VITE_DEV_MODE=true
VITE_DEBUG_MODE=false

# 文件上传配置
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,image/webp

# 实时功能配置
VITE_ENABLE_REALTIME=true
VITE_REALTIME_EVENTS_PER_SECOND=10

# 缓存配置
VITE_ENABLE_OFFLINE_CACHE=true
VITE_CACHE_DURATION=300000

# 功能开关
VITE_ENABLE_CHAT_HISTORY=true
VITE_ENABLE_FILE_UPLOAD=true
VITE_ENABLE_POMODORO=true
VITE_ENABLE_PROJECTS=true

# 主题配置
VITE_DEFAULT_THEME=light
VITE_ENABLE_THEME_SWITCHING=true

# 性能配置
VITE_BATCH_SIZE=100
VITE_REQUEST_TIMEOUT=30000
VITE_RETRY_ATTEMPTS=3

# 安全配置
VITE_ENABLE_CSP=true
VITE_ENABLE_HTTPS_ONLY=false

# 本地化配置
VITE_DEFAULT_LOCALE=zh-CN
VITE_ENABLE_I18N=false

# 备份和同步配置
VITE_ENABLE_AUTO_BACKUP=true
VITE_BACKUP_INTERVAL=3600000

# 通知配置
VITE_ENABLE_NOTIFICATIONS=true
VITE_NOTIFICATION_TIMEOUT=5000

# 搜索配置
VITE_SEARCH_DEBOUNCE_DELAY=300
VITE_SEARCH_MIN_CHARS=2

# 编辑器配置
VITE_EDITOR_AUTO_SAVE=true
VITE_EDITOR_AUTO_SAVE_DELAY=2000

# 导出配置
VITE_ENABLE_EXPORT=true
VITE_EXPORT_FORMATS=json,csv,md